(deftype Unit Type
  unit Unit)

(deftype Nat Type
  Z Nat
  S (-> Nat Nat))

(deftype (Pair T U) (-> Type Type Type)
  pair (-> T U (Pair T U)))

(deftype (List T) (-> Type Type)
  nil (List T)
  cons (-> T (List T) (List T)))
|#


(defun id+ ((T Type) (x T)) T
  x)

(defun const+ ((T Type) (U Type) (x T) (y U)) T
  x)



